<h1>Login</h1>
<%= form_tag sessions_path do |f| %>
  <div class="input-field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div><br>
  <div class="input-field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div><br>
  <div>
    <%= submit_tag nil, class: "waves-effect waves-red btn left"%>
  </div>
<% end %>
